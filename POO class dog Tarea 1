class Dog:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def bark(self):
        return f"{self.name} says woof!"

    def get_age_in_dog_years(self):
        return self.age * 7

    def __str__(self):
        return f"{self.name} is {self.age} years old."

# Creación de un objeto Dog
my_dog = Dog("Rex", 5)

# Llamada a los métodos
print(my_dog.bark())  # Rex says woof!
print(my_dog.get_age_in_dog_years())  # 35
print(my_dog)  # Rex is 5 years old.
